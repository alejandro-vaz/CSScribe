{
    "scopeName": "source.fmd",
    "patterns": [
        {
            "name": "fmd.bold.fmd",
            "begin": "'",
            "patterns": [
                { "include": "#italic" },
                { "include": "#strikethrough" },
                { "include": "#superscript" },
                { "include": "#subscript" },
                { "include": "#scape"}
            ],
            "end": "'"
        },
        {
            "name": "fmd.italic.fmd",
            "begin": "·",
            "patterns": [
                { "include": "#bold" },
                { "include": "#strikethrough" },
                { "include": "#superscript" },
                { "include": "#subscript" },
                { "include": "#scape"}
            ],
            "end": "·"
        },
        {
            "name": "fmd.strikethrough.fmd",
            "begin": "/-",
            "patterns": [
                { "include": "#bold" },
                { "include": "#italic" },
                { "include": "#superscript" },
                { "include": "#subscript" },
                { "include": "#scape"}
            ],
            "end": "/-"
        },
        {
            "name": "fmd.subscript.fmd",
            "match": "/s"
        },
        {
            "name": "fmd.superscript.fmd",
            "match": "/S"
        },
        {
            "name": "fmd.code.fmd",
            "begin": "/code-",
            "end": "/code"
        },
        {
            "name": "fmd.newline.fmd",
            "match": "/n"
        },
        {
            "name": "fmd.comment.fmd",
            "begin": "#>",
            "end": "<#"
        },
        {
            "name": "fmd.icode.fmd",
            "begin": "/cd",
            "end": "/cd"
        },
        {
            "name": "fmd.h1.fmd",
            "begin": "/h1",
            "patterns": [
                { "include": "#newline" },
                { "include": "#scape"}
            ],
            "end": "$"
        },
        {
            "name": "fmd.h2.fmd",
            "begin": "/h2",
            "patterns": [
                { "include": "#newline" },
                { "include": "#scape"}
            ],
            "end": "$"
        },
        {
            "name": "fmd.h3.fmd",
            "begin": "/h3",
            "patterns": [
                { "include": "#newline" },
                { "include": "#scape"}
            ],
            "end": "$"
        },
        {
            "name": "fmd.img.fmd",
            "match": "/img"
        },
        {
            "name": "fmd.side.fmd",
            "begin": "/>",
            "patterns": [
                { "include": "#newline" },
                { "include": "#subscript" },
                { "include": "#superscript" },
                { "include": "#imath" },
                { "include": "#bold" },
                { "include": "#italic" },
                { "include": "#strikethrough" },
                { "include": "#ipy" },
                { "include": "#scape"}
            ],
            "end": "$"
        },
        {
            "name": "fmd.void.fmd",
            "match": "/O"
        },
        {
            "name": "fmd.done.fmd",
            "match": "/X"
        },
        {
            "name": "fmd.ol.fmd",
            "match": "    o "
        },
        {
            "name": "fmd.ul.fmd",
            "match": "    * "
        },
        {
            "name": "fmd.brochet.fmd",
            "match": "[\\(\\)\\[\\]\\{\\}]"
        },
        {
            "name": "fmd.imath.fmd",
            "begin": "/_",
            "patterns": [
                { "include": "#scape"}
            ],
            "end": "/_"
        },
        {
            "name": "fmd.math.fmd",
            "begin": "/math",
            "patterns": [
                { "include": "#scape"}
            ],
            "end": "/math"
        },
        {
            "name": "fmd.pbba.fmd",
            "match": "/pbba"
        },
        {
            "name": "fmd.scape.fmd",
            "match": "¬"
        },
        {
            "begin": "/In",
            "end": "/in",
            "name": "fmd.index.fmd"
        },
        {
            "name": "fmd.break.fmd",
            "match": "/br"
        }
    ],
    "repository": {
        "cd": {
            "name": "fmd.code.fmd",
            "begin": "/cd",
            "end": "/cd"
        },
        "imath": {
            "name": "fmd.imath.fmd",
            "begin": "/_",
            "end": "/_"
        },
        "superscript": {
            "name": "fmd.superscript.fmd",
            "match": "/S"
        },
        "subscript": {
            "name": "fmd.subscript.fmd",
            "match": "/s"
        },
        "bold": {
            "name": "fmd.bold.fmd",
            "begin": "'",
            "end": "'"
        },
        "italic": {
            "name": "fmd.italic.fmd",
            "begin": "·",
            "end": "·"
        },
        "strikethrough": {
            "name": "fmd.strikethrough.fmd",
            "begin": "/-",
            "end": "/-"
        },
        "newline": {
            "name": "fmd.newline.fmd",
            "match": "/n"
        },
        "scape": {
            "name": "fmd.scape.fmd",
            "match": "¬"
        }
    }
}